{
  "name": "app-launcher",
  "version": "1.1.3",
  "description": "一个用于在浏览器中唤起移动端App的NPM包，支持多种平台和智能策略",
  "author": "BluesYoung-web",
  "license": "MIT",
  "homepage": "https://github.com/BluesYoung-web/app-launcher#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BluesYoung-web/app-launcher.git"
  },
  "bugs": {
    "url": "https://github.com/BluesYoung-web/app-launcher/issues"
  },
  "keywords": [
    "call-app",
    "app-launcher",
    "browser",
    "mobile",
    "deeplink",
    "url-scheme",
    "universal-links"
  ],
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "init.mjs"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite --host",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepublishOnly": "pnpm run build",
    "postinstall": "node ./init.mjs"
  },
  "dependencies": {
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^0.34.0",
    "@vitest/ui": "^0.34.0",
    "eslint": "^9.35.0",
    "jsdom": "^22.0.0",
    "lint-staged": "^16.1.6",
    "simple-git-hooks": "^2.13.1",
    "terser": "^5.44.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^0.34.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
